{
  "actions" : {
    "build" : {
      "postActions" : [
        {
          "script" : "source env\/bin\/activate",
          "type" : "runScript"
        }
      ]
    },
    "clean" : {
      "postActions" : [
        {
          "script" : "deactivate",
          "type" : "runScript"
        }
      ]
    }
  },
  "extension" : {
    "identifier" : "panic.Python",
    "name" : "Python"
  },
  "extensionTemplate" : "pythonDebug",
  "extensionValues" : {
    "python.debug.script" : "index.py"
  },
  "openLogOnRun" : "fail"
}
